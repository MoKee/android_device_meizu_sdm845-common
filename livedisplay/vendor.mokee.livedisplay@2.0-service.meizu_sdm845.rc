service vendor.livedisplay-hal-2-0-meizu_sdm845 /vendor/bin/hw/vendor.mokee.livedisplay@2.0-service.meizu_sdm845
    class late_start
    user system
    group system

on post-fs-data
    mkdir /data/vendor/display
    chown system system /data/vendor/display

on property:sys.boot_completed=1
    exec - system system -- /vendor/bin/vendor.mokee.livedisplay@2.0-init.sh
